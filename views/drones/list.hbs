<h2>Available drones</h2>
<ul>
{{#each allDrones}}
    <li>
        <p>Name: {{this.name}}</p>
        <p>Propellers: {{this.propellers}}</p>
        <p>Speed: {{this.maxSpeed}}</p>
        <a href="/drones/{{this._id}}/edit">Edit</a>
        <form action="/drones/{{this.id}}/delete" method="post"><button>Delete</button></form>
    </li>
{{/each}}
</ul>